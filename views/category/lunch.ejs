<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Document</title>
</head>
<body>
  <header id="header">
    <h1>
      Reci.P
    </h1>
  </header>
  <main>
    <div class="container">
      <div>
        <h3>Lunch</h3>
        <ul>
          <% for (let i = 0; i < lunchData.recipes.length; i++) { %>
            <li>
              <a href="/recipes/<%= lunchData.recipes[i]._id%>">
                <%= lunchData.recipes[i].title%></a>
            </li>
          <% } %>
        </ul>
      </div>
    </div>
    <form action="/recipes/addRecipe" method="GET">
      <button type="submit">Add Recipe</button>
  </form>
  <form action="/categories" method="GET">
    <button type="submit">Back To Categories</button>
</form>
  </main>
</body>
</html>